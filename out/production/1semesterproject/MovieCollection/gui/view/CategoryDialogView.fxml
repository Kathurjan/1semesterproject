<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<DialogPane expanded="true" prefHeight="136.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MovieCollection.gui.controller.CategoryDialogController">
   <content>
      <HBox prefHeight="38.0" prefWidth="360.0">
         <children>
            <Label text="Category Name">
               <font>
                  <Font size="14.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="txtFieldCategory">
               <HBox.margin>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="APPLY" />
   </buttonTypes>
</DialogPane>
