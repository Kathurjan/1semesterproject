   <buttonTypes>
      <ButtonType fx:constant="CLOSE" />
   </buttonTypes>
   <content>
      <HBox prefHeight="159.0" prefWidth="398.0">
         <children>
            <ListView fx:id="lstViewCategories" prefHeight="200.0" prefWidth="200.0" />
            <Button fx:id="btnDeleteCategory" mnemonicParsing="false" onAction="#handleDeleteCategory" text="Delete">
               <HBox.margin>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </content>
</DialogPane>
